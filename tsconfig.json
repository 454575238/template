{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "lib": ["es6", "es7", "dom", "ES2015.Collection", "ES2015.Iterable"],
    "outDir": "./dist/",
    "noImplicitAny": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true
  },
  "exclude": ["node_modules", "dist", "build"]
}
